<xml>
    <processstep name="Cameras" group="Camera configuration">
        <parameter name="Camera Selection" type="selectable">
            <option>Camera 1 & 2</option>
            <option>Camera 1 & 3</option>
            <option>Camera 1 & 4</option>
            <option>Camera 2 & 3</option>
            <option>Camera 2 & 4</option>
            <option>Camera 3 & 4</option>
            <option>Camera 5 & 6</option>
            <option>Camera 5 & 7</option>
            <option>Camera 5 & 8</option>
            <option>Camera 6 & 7</option>
            <option>Camera 6 & 8</option>
            <option>Camera 7 & 8</option>
        </parameter>
        <parameter name="Shot mode" type="selectable">
            <option>Single shot mode</option>
            <option>Continous mode</option>
        </parameter>
        <parameter name="hSize" type="numeric" min="320" init="640" max="1920" >
        <parameter name="vSize" type="numeric" min="240" init="480" max="1080" >
    </processstep>

    <processstep name="Equalisation" group="Camera calibration">
        <parameter name="Enable" type="boolean">
        </parameter>
    </processstep>

    <processstep name="Rectification" group="Camera calibration">
        <parameter name="Enable" type="boolean">
        </parameter>
    </processstep>

</xml>
